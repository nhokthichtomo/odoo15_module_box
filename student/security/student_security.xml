<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">
        <record id="base.module_student" model="ir.module.category">
            <field name="description">Helps you manage your student for review of different-different users.</field>
            <field name="sequence">20</field>
        </record>
        
        <record id="ir_rule_student_view" model="ir.rule">
            <field name="name">Student</field>
            <field name="model_id" ref="base.module_student"/>
            <field name="global" eval="True"/>
            <field name="groups" eval="[(4, ref('base.group_user'))]"/>
            <field name="domain_force">[('name','=',True)]</field>
            <field name="perm_unlink" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_create" eval="1"/>            
        </record>

    </data>
</odoo>